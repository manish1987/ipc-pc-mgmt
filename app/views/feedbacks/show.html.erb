<%- model_class = Feedback -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:session_id) %>:</strong></dt>
  <dd><%= @feedback.session_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:feedback) %>:</strong></dt>
  <dd><%= @feedback.feedback %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              feedbacks_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_feedback_path(@feedback), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              feedback_path(@feedback),
              :method => 'delete',
              :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')),
              :class => 'btn btn-danger' %>
</div>
